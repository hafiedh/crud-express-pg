<%- include('./components/head') %> <%- include('./components/navbar') %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">No.</th>
      <th scope="col">Name</th>
      <th scope="col">Age</th>
      <th scope="col">keahlian</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < data.length; i++) { %>
    <tr>
      <th scope="row "><%= i + 1 %></th>
      <td><%= data[i].nama %></td>
      <td><%= data[i].umur %></td>
      <td><%= data[i].keahlian %></td>
    </tr>
    <% } %>
  </tbody>
</table>
<form action="instructor/redirect" method="post">
  <div class="container my-3 bg-light">
        <div class="col-md-12 text-center">
            <input type="submit" class="btn btn-primary" value="Redirect To Peserta"></input>
    </div>
  </button>
</form>
